{
  "version": "0.0.12",
  "version_name": "tn-0.0.12#68.0_GC-w7",
  "name": "yoroi",
  "manifest_version": 2,
  "description": "[testnet] Cardano ADA wallet",
  "browser_action": {
    "default_title": "[testnet] Yoroi"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "contextMenus",
    "management",
    "tabs",
    "storage",
    "*://connect.trezor.io/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://connect.trezor.io/*/popup.html"],
      "js": ["js/trezor-content-script.js"]
    }
  ],
  "content_security_policy": "default-src 'self'; frame-src 'self' https://connect.trezor.io/; script-src 'self' 'unsafe-eval'; connect-src https://18.207.22.26:8080 wss://18.207.22.26:8080; style-src * 'unsafe-inline' 'self' blob:; img-src 'self' data:;"
}
